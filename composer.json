{
	"name": "sallycms/sallycms",
	"repositories": [{
		"type": "package",
		"package": {
			"name": "fabpot/yaml",
			"version": "1.0.6",
			"autoload": {"psr-0":{"sfYaml": ""}},
			"source": {
				"url": "https://github.com/fabpot/yaml",
				"type": "git",
				"reference": "master"
			}
		}
	}, {
		"type": "package",
		"package": {
			"name": "leafo/lessphp",
			"version": "0.3.4",
			"autoload": {"classmap": "./"},
			"source": {
				"url": "https://github.com/leafo/lessphp",
				"type": "git",
				"reference": "master"
			}
		}
	}],
	"require": {
		"php": ">=5.2.1",
		"fabpot/yaml": "1.*",
		"leafo/lessphp": "0.3.*",
		"webvariants/babelcache": "1.2.*"
	},
	"config": {
		"vendor-dir": "sally/vendor"
	}
}
